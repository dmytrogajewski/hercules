expression <- (expression) => uast(
    type: "Synthetic"
)

statement <- (statement) => uast(
    type: "Synthetic"
)

array_access <- (array_access) => uast(
    type: "Synthetic",
    children: "expression"
)

assignment_statement <- (assignment_statement) => uast(
    type: "Assignment",
    roles: "Assignment"
)

binary_expression <- (binary_expression) => uast(
    type: "Synthetic",
    children: "expression"
)

block <- (block) => uast(
    type: "Block",
    roles: "Body"
)

boolean <- (boolean) => uast(
    type: "Synthetic"
)

call_expression <- (call_expression) => uast(
    type: "Call",
    roles: "Call",
    children: "identifier"
)

else_statement <- (else_statement) => uast(
    type: "Synthetic",
    children: "block", "if_statement"
)

expansion <- (expansion) => uast(
    type: "Synthetic"
)

foreach_statement <- (foreach_statement) => uast(
    type: "Loop",
    roles: "Loop",
    children: "expression", "identifier"
)

if_statement <- (if_statement) => uast(
    type: "If",
    children: "block", "expression"
)

import_statement <- (import_statement) => uast(
    type: "Import",
    roles: "Import"
)

list <- (list) => uast(
    type: "List"
)

parenthesized_expression <- (parenthesized_expression) => uast(
    type: "Synthetic"
)

primary_expression <- (primary_expression) => uast(
    type: "Synthetic"
)

scope_access <- (scope_access) => uast(
    type: "Synthetic",
    children: "expression", "identifier"
)

source_file <- (source_file) => uast(
    type: "Synthetic"
)

string <- (string) => uast(
    type: "Synthetic"
)

string_content <- (string_content) => uast(
    type: "Synthetic"
)

unary_expression <- (unary_expression) => uast(
    type: "Synthetic"
)

comment <- (comment) => uast(
    type: "Comment",
    roles: "Comment"
)

else <- (else) => uast(
    type: "Synthetic"
)

escape_sequence <- (escape_sequence) => uast(
    type: "Synthetic"
)

false <- (false) => uast(
    type: "Synthetic"
)

foreach <- (foreach) => uast(
    type: "Loop",
    roles: "Loop"
)

hex <- (hex) => uast(
    type: "Synthetic"
)

identifier <- (identifier) => uast(
    type: "If"
)

if <- (if) => uast(
    type: "If"
)

import <- (import) => uast(
    type: "Import",
    roles: "Import"
)

integer <- (integer) => uast(
    type: "Synthetic"
)

true <- (true) => uast(
    type: "Synthetic"
)

