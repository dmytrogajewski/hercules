array <- (array) => uast(
    type: "Synthetic"
)

block <- (block) => uast(
    type: "Block",
    roles: "Body",
    children: "identifier"
)

else_clause <- (else_clause) => uast(
    type: "Synthetic"
)

expression <- (expression) => uast(
    type: "Synthetic"
)

for_statement <- (for_statement) => uast(
    type: "Loop",
    roles: "Loop"
)

function <- (function) => uast(
    type: "Function",
    roles: "Function", "Declaration",
    children: "array", "identifier", "key_value_pair", "parameter"
)

if_statement <- (if_statement) => uast(
    type: "If",
    children: "else_clause", "expression", "statement"
)

interpolation <- (interpolation) => uast(
    type: "Synthetic"
)

key_value_pair <- (key_value_pair) => uast(
    type: "KeyValue",
    roles: "Key", "Value",
    children: "array", "string"
)

operator <- (operator) => uast(
    type: "Synthetic"
)

parameter <- (parameter) => uast(
    type: "Parameter",
    roles: "Parameter",
    children: "identifier"
)

parser <- (parser) => uast(
    type: "Synthetic"
)

property <- (property) => uast(
    type: "Property",
    roles: "Member",
    children: "identifier", "string"
)

statement <- (statement) => uast(
    type: "Synthetic"
)

string <- (string) => uast(
    type: "Synthetic"
)

and <- (and) => uast(
    type: "Synthetic"
)

comment <- (comment) => uast(
    type: "Comment",
    roles: "Comment"
)

else <- (else) => uast(
    type: "Synthetic"
)

escaped_char <- (escaped_char) => uast(
    type: "Synthetic"
)

for <- (for) => uast(
    type: "Loop",
    roles: "Loop"
)

identifier <- (identifier) => uast(
    type: "If"
)

if <- (if) => uast(
    type: "If"
)

in <- (in) => uast(
    type: "Synthetic"
)

map <- (map) => uast(
    type: "Synthetic"
)

or <- (or) => uast(
    type: "Synthetic"
)

