comment <- (comment) => uast(
    type: "Comment",
    roles: "Comment"
)

directive <- (directive) => uast(
    type: "Synthetic"
)

expression <- (expression) => uast(
    type: "Synthetic"
)

fragment <- (fragment) => uast(
    type: "Synthetic"
)

partial_expression <- (partial_expression) => uast(
    type: "Synthetic"
)

do <- (do) => uast(
    type: "Synthetic"
)

text <- (text) => uast(
    type: "Synthetic"
)

