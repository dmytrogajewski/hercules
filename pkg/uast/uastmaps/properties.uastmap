[language "properties", extensions: ".properties"]

escape <- (escape) => uast(
    type: "Synthetic"
)

file <- (file) => uast(
    type: "Synthetic"
)

index <- (index) => uast(
    type: "Index",
    roles: "Index"
)

key <- (key) => uast(
    type: "Synthetic"
)

property <- (property) => uast(
    type: "Property",
    roles: "Member"
)

secret <- (secret) => uast(
    type: "Synthetic"
)

substitution <- (substitution) => uast(
    type: "Synthetic",
    children: "escape", "substitution"
)

value <- (value) => uast(
    type: "Synthetic"
)

comment <- (comment) => uast(
    type: "Comment",
    roles: "Comment"
)